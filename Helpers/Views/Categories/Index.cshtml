@model List<Helpers.Models.Category>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Categories</h2>
@Html.ActionLink("Create","Create","Categories")
<table class="table">
    <tr>
        <th>Category ID</th>
        <th>Category Name</th>
    </tr>
    @foreach(var item in Model)
     {
        <tr>
            <td>@Html.DisplayFor(temp=>item.CategoryID)</td>
            <td>@Html.DisplayFor(temp=>item.CategoryName)</td>
        </tr>
     }
</table>

